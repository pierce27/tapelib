<h1>
<%= session[:user_name] %>
's Saved Tapes
</h1>

<table>
  <tr>
    <th>Voltag</th>
    <th>Notes</th>
  </tr>

<% @saved_tapes.each do |saved_tape| %>
  <tr>
    <td><%=h saved_tape.voltag %></td>
    <td><%=h saved_tape.notes %></td>
    <td><%= button_to 'View', :voltag => saved_tape.voltag, :action => :yourtape %></td>
    <td><%= link_to 'Edit', edit_saved_tape_path(saved_tape) %></td>
    <td><%= button_to 'Destroy', :action => 'destroy', :id => saved_tape.id %></td>
  </tr>
<% end %>
</table>

<br />

<%= button_to "Search", :controller => "tapes", :action => "search"%>
<%= button_to_function 'Back', "history.back()" %>
