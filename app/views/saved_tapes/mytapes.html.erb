<h1>Listing saved_tapes</h1>

<table>
  <tr>
    <th>Voltag</th>
    <th>Notes</th>
  </tr>

<% @saved_tapes.each do |saved_tape| %>
  <tr>
    <td><%=h saved_tape.voltag %></td>
    <td><%=h saved_tape.notes %></td>
    <td><%= link_to 'Show', saved_tape %></td>
    <td><%= link_to 'Edit', edit_saved_tape_path(saved_tape) %></td>
    <td><%= link_to 'Destroy', :action => 'destroy', :id => saved_tape.id %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Add', new_saved_tape_path %>
<%= link_to 'Back', :controller => 'user', :action => 'private' %>
